{
  "name": "n8n-nodes-peek-pro",
  "version": "0.1.0",
  "description": "n8n community node to work with the Example API",
  "license": "MIT",
  "homepage": "https://github.com/peek-travel/n8n-nodes-peek-pro#readme",
  "keywords": [
    "n8n",
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Oskar Bruening",
    "email": "oskar@peek.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peek-travel/n8n-nodes-peek-pro"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PeekProApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PeekPro/PeekPro.node.js",
      "dist/nodes/PeekProTrigger/PeekProTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.13.0",
    "eslint": "9.32.0",
    "n8n-core": "^1.114.0",
    "n8n-workflow": "^1.112.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  }
}
